{% extends 'base.html.twig' %}

{% block title %}SampleFile{% endblock %}

{% block body %}
    <h1>SampleFile</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sample_file.id }}</td>
            </tr>
            <tr>
                <th>FileName</th>
                <td>{{ sample_file.fileName }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ sample_file.updatedAt ? sample_file.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ sample_file.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sample_file_index') }}">back to list</a>

    <a href="{{ path('sample_file_edit', {'id': sample_file.id}) }}">edit</a>

    {{ include('sample_file/_delete_form.html.twig') }}
{% endblock %}
